void boxOfStars(int width, int height){
    for(int i=1;i<=height;++i){
        for(int j=1;j<=width;++j){
            if(i==1 || i==height){
                cout<<"*";
            }
            else{
                if(j==1 || j==width){
                    cout<<"*";
                }
                else cout<<" ";
            }
        }
        cout<<"\n";
    }
}
